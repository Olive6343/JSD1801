<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name></display-name>
  <servlet>
    <servlet-name>uploadServlet</servlet-name>
    <servlet-class>net.line.servlet.UploadServlet</servlet-class>
  </servlet>
  <servlet>
    <description>UserServlet</description>
    <display-name>UserServlet</display-name>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>net.line.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <description>GoodsManager</description>
    <display-name>GoodsManager</display-name>
    <servlet-name>GoodsManager</servlet-name>
    <servlet-class>net.line.servlet.GoodsManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SalerServlet</servlet-name>
    <servlet-class>net.line.servlet.SalerServlet</servlet-class>
  </servlet>
  <servlet>
    <description>GoodsServlet</description>
    <display-name>GoodsServlet</display-name>
    <servlet-name>GoodsServlet</servlet-name>
    <servlet-class>net.line.servlet.GoodsServlet</servlet-class>
  </servlet>
  <servlet>
    <description>InStoreServlet</description>
    <display-name>InStoreServlet</display-name>
    <servlet-name>InStoreServlet</servlet-name>
    <servlet-class>net.line.servlet.InStoreServlet</servlet-class>
  </servlet>
  <servlet>
    <description>SaleOutServlet</description>
    <display-name>SaleOutServlet</display-name>
    <servlet-name>SaleOutServlet</servlet-name>
    <servlet-class>net.line.servlet.SaleOutServlet</servlet-class>
  </servlet>
  <servlet>
    <description>IdentityServlet</description>
    <display-name>IdentityServlet</display-name>
    <servlet-name>IdentityServlet</servlet-name>
    <servlet-class>net.line.common.IdentityServlet</servlet-class>
  </servlet>
  <filter>
    <filter-name>LoginFilter</filter-name>
    <filter-class>net.line.core.web.filter.LoginFilter</filter-class>
    <init-param>
      <param-name>user</param-name>
      <param-value>login.jsp</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/page/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>StoreTypeServlet</servlet-name>
    <servlet-class>net.line.servlet.StoreTypeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SupplierServlet</servlet-name>
    <servlet-class>net.line.servlet.SupplierServlet</servlet-class>
  </servlet>
  <filter>
    <filter-name>IndexJSPFilter</filter-name>
    <filter-class>net.line.core.web.filter.IndexJSPFilter</filter-class>
    <init-param>
      <param-name>user2</param-name>
      <param-value>/index.jsp</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>IndexJSPFilter</filter-name>
    <url-pattern>/index.jsp</url-pattern>
  </filter-mapping>
  <servlet-mapping>
    <servlet-name>SupplierServlet</servlet-name>
    <url-pattern>/servlet/SupplierServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>StoreTypeServlet</servlet-name>
    <url-pattern>/servlet/StoreTypeServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SaleOutServlet</servlet-name>
    <url-pattern>/servlet/SaleOutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InStoreServlet</servlet-name>
    <url-pattern>/servlet/InStoreServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SalerServlet</servlet-name>
    <url-pattern>/servlet/SalerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>uploadServlet</servlet-name>
    <url-pattern>/servlet/uploadServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/servlet/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GoodsManager</servlet-name>
    <url-pattern>/servlet/GoodsManager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GoodsServlet</servlet-name>
    <url-pattern>/servlet/GoodsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IdentityServlet</servlet-name>
    <url-pattern>/common/IdentityServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>